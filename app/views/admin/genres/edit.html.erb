<div class = "container">
  <div clas = "row">
    <div class = "col-12">
      <h3>ジャンルの編集画面</h3>
      <%= form_with model: @genre, url: admin_genre_path do |f| %>
      <%= f.text_field :name, class: 'form-control' %>
      <%= f.submit '更新', class: "btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>